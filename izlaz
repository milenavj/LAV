-  int b = 0;
+ int b = 0;
 
   for (unsigned i = 0; i < _Functions.size(); i++)
     if (_Functions[i]->GetFunctionName() == StartFunction) {
@@ -66,18 +89,61 @@ void LModule::CalculateDescriptions() {
